{
  "name": "@deepverse/zero-ui",
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/button/zui-button.d.ts",
      "default": "./dist/button/zui-button.js"
    },
    "./card": {
      "types": "./dist/card/zui-card.d.ts",
      "default": "./dist/card/zui-card.js"
    },
    "./file-upload": {
      "types": "./dist/file-upload/zui-file-upload.d.ts",
      "default": "./dist/file-upload/zui-file-upload.js"
    },
    "./otp-input": {
      "types": "./dist/otp-input/zui-otp-input.d.ts",
      "default": "./dist/otp-input/zui-otp-input.js"
    },
    "./phone-input": {
      "types": "./dist/phone-input/zui-phone-input.d.ts",
      "default": "./dist/phone-input/zui-phone-input.js"
    },
    "./star-rating": {
      "types": "./dist/star-rating/zui-star-rating.d.ts",
      "default": "./dist/star-rating/zui-star-rating.js"
    },
    "./select": {
      "types": "./dist/select/zui-select.d.ts",
      "default": "./dist/select/zui-select.js"
    },
    "./dropdown": {
      "types": "./dist/dropdown/zui-dropdown.d.ts",
      "default": "./dist/dropdown/zui-dropdown.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "lit": "^3.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "rollup": "^4.53.3",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.9.3"
  }
}
